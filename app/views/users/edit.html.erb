<div class="edit">
<h5>Edit Profile</h5>
<%= form_for @user do |f| %>
  <label for="input_text">First Name:</label>
  <%= f.text_field :first_name %>
  <label for="input_text">Last Name:</label>
  <%= f.text_field :last_name %>
  <label for="input_text">Course Taken at GA:</label>
  <%= f.collection_select :course_taken, User::COURSE, :to_s, :to_s, :include_blank => false, :prompt => "Select Option" %><br />
  <label for="input_text">Campus:</label>
  <%= f.collection_select :city, User::CITIES, :to_s, :to_s, :include_blank => false, :prompt => "Select Option" %><br />
  <label for="input_text">Email:</label>
  <%= f.text_field :email %>
  <label for="input_text">User Name:</label>
  <%= f.text_field :user_name %>
  <label for="input_text">Profile Picture:</label>
  <%= f.text_field :image_url %>
  <%= f.submit "Submit", :class=>'waves-effect waves-red waves-ripple btn' %>
<% end %>
<br />
</div>
